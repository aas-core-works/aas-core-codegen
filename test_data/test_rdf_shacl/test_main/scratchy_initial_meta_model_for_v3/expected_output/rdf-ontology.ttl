<https://admin-shell.io/aas/3/0/RC01/> rdf:type owl:Ontology ;
    vann:preferredNamespaceUri "https://admin-shell.io/aas/3/0/RC01/"^^xsd:anyURI ;
    owl:versionInfo "3.0.RC01" ;
    rdfs:comment "This ontology represents the data model for the Asset Administration Shell according to the specification 'Details of the Asset Administration Shell - Part 1 - Version 3.0.RC01'."@en ;
    skos:prefLabel "aas"^^xsd:string ;
    vann:preferredNamespacePrefix "aas"^^xsd:string ;
    rdfs:isDefinedBy <https://admin-shell.io/aas/3/0/RC01/> ;
.

### https://admin-shell.io/aas/3/0/RC01/HasExtension
aas:HasExtension rdf:type owl:Class ;
    rdfs:label "Has Extension"^^xsd:string ;
.

### https://admin-shell.io/aas/3/0/RC01/LangString
aas:LangString rdf:type owl:Class ;
    rdfs:comment "Give a text in a specific language."@en ;
    rdfs:label "Lang String"^^xsd:string ;
.

### https://admin-shell.io/aas/3/0/RC01/LangStringSet
aas:LangStringSet rdf:type owl:Class ;
    rdfs:comment """A set of strings, each annotated by the language of the string.

The meaning of the string in each language shall be the same."""@en ;
    rdfs:label "Lang String Set"^^xsd:string ;
.

### https://admin-shell.io/aas/3/0/RC01/Referable
aas:Referable rdf:type owl:Class ;
    rdfs:comment """An element that is referable by its 'idShort'.

This identifier is not globally unique.
This identifier is unique within the name space of the element."""@en ;
    rdfs:label "Referable"^^xsd:string ;
.

