        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="^[a-zA-Z_][a-zA-Z_0-9]*$"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="displayName" type="langStringSet_t" minOccurs="0"/>
      <xs:element name="category" type="xs:string" minOccurs="0"/>
      <xs:element name="description" type="langStringSet_t" minOccurs="0"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="referable_abstract">
    <xs:choice>
      <xs:element name="assetAdministrationShell" type="assetAdministrationShell_t"/>
      <xs:element name="asset" type="asset_t"/>
      <xs:element name="submodel" type="submodel_t"/>
      <xs:element name="submodelElementCollection" type="submodelElementCollection_t"/>
      <xs:element name="property" type="property_t"/>
      <xs:element name="multiLanguageProperty" type="multiLanguageProperty_t"/>
      <xs:element name="range" type="range_t"/>
      <xs:element name="referenceElement" type="referenceElement_t"/>
      <xs:element name="blob" type="blob_t"/>
      <xs:element name="file" type="file_t"/>
      <xs:element name="annotatedRelationshipElement" type="annotatedRelationshipElement_t"/>
      <xs:element name="entity" type="entity_t"/>
      <xs:element name="basicEvent" type="basicEvent_t"/>
      <xs:element name="operation" type="operation_t"/>
      <xs:element name="capability" type="capability_t"/>
      <xs:element name="conceptDescription" type="conceptDescription_t"/>
      <xs:element name="view" type="view_t"/>
      <xs:element name="accessPermissionRule" type="accessPermissionRule_t"/>
    </xs:choice>
  </xs:group>
  <xs:group name="identifiable_t">
    <xs:sequence>
      <xs:group ref="referable_t"/>
      <xs:element name="administration" type="administrativeInformation_t" minOccurs="0"/>
      <xs:element name="identification" type="identifier_t"/>
    </xs:sequence>
  </xs:group>
  <xs:complexType name="identifier_t">
    <xs:sequence>
      <xs:element name="idType" type="identifierType_t"/>
      <xs:element name="id" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="identifierType_t">
    <xs:restriction base="xs:string">
      <xs:enumeration value="IRDI"/>
      <xs:enumeration value="IRI"/>
      <xs:enumeration value="Custom"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="modelingKind_t">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Template"/>
      <xs:enumeration value="Instance"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:group name="hasKind_t">
    <xs:sequence>
      <xs:element name="kind" type="modelingKind_t" minOccurs="0"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="hasDataSpecification_t">
    <xs:sequence>
      <xs:element name="dataSpecifications" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="reference" type="reference_t"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:complexType name="administrativeInformation_t">
    <xs:sequence>
      <xs:group ref="hasDataSpecification_t"/>
      <xs:element name="version" type="xs:string" minOccurs="0"/>
      <xs:element name="revision" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:group name="qualifiable_t">
    <xs:sequence>
      <xs:element name="qualifiers" minOccurs="0">
        <xs:complexType>
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:group ref="constraint_abstract"/>
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="constraint_t">
    <xs:sequence/>
  </xs:group>
  <xs:group name="constraint_abstract">
    <xs:choice>
      <xs:element name="qualifier" type="qualifier_t"/>
      <xs:element name="formula" type="formula_t"/>
    </xs:choice>
  </xs:group>
  <xs:complexType name="qualifier_t">
    <xs:sequence>
      <xs:group ref="constraint_t"/>
      <xs:group ref="hasSemantics_t"/>
      <xs:element name="type" type="xs:string"/>
      <xs:element name="valueType" type="dataTypeDef_t"/>
      <xs:element name="value" type="xs:string" minOccurs="0"/>
      <xs:element name="valueId" type="reference_t" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="formula_t">
    <xs:sequence>
      <xs:group ref="constraint_t"/>
      <xs:element name="dependsOn" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="reference" type="reference_t"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="assetAdministrationShell_t">
    <xs:sequence>
      <xs:group ref="identifiable_t"/>
      <xs:group ref="hasDataSpecification_t"/>
      <xs:element name="derivedFrom" type="reference_t" minOccurs="0"/>
      <xs:element name="security" type="security_t" minOccurs="0"/>
      <xs:element name="assetInformation" type="assetInformation_t"/>
      <xs:element name="submodels" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="reference" type="reference_t"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="views" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="view" type="view_t"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="asset_t">
    <xs:sequence>
      <xs:group ref="identifiable_t"/>
      <xs:group ref="hasDataSpecification_t"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="assetInformation_t">
    <xs:sequence>
      <xs:element name="assetKind" type="assetKind_t"/>
      <xs:element name="globalAssetId" type="reference_t" minOccurs="0"/>
      <xs:element name="specificAssetIds" type="identifierKeyValuePair_t" minOccurs="0"/>
