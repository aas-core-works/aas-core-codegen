        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="reference" type="reference_t"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="defaultThumbnail" type="file_t" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="assetKind_t">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Type"/>
      <xs:enumeration value="Instance"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="identifierKeyValuePair_t">
    <xs:sequence>
      <xs:group ref="hasSemantics_t"/>
      <xs:element name="key" type="xs:string"/>
      <xs:element name="value" type="xs:string" minOccurs="0"/>
      <xs:element name="externalSubjectId" type="reference_t" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="submodel_t">
    <xs:sequence>
      <xs:group ref="identifiable_t"/>
      <xs:group ref="hasKind_t"/>
      <xs:group ref="hasSemantics_t"/>
