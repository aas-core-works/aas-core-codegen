<?xml version="1.0" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://www.admin-shell.io/aas/3/0">
  <xs:group name="hasSemantics_t">
    <xs:sequence>
      <xs:element name="semanticId" type="reference_t" minOccurs="0"/>
    </xs:sequence>
  </xs:group>
  <xs:complexType name="extension_t">
    <xs:sequence>
      <xs:group ref="hasSemantics_t"/>
      <xs:element name="name" type="xs:string"/>
      <xs:element name="valueType" type="dataTypeDef_t" minOccurs="0"/>
      <xs:element name="value" type="xs:string" minOccurs="0"/>
      <xs:element name="refersTo" type="reference_t" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:group name="hasExtensions_t">
    <xs:sequence>
      <xs:element name="extension" type="extension_t" minOccurs="0"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="referable_t">
    <xs:sequence>
      <xs:group ref="hasExtensions_t"/>
