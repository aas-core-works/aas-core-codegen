      <xs:group ref="hasDataSpecification_t"/>
      <xs:element name="submodelElements" minOccurs="0">
        <xs:complexType>
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:group ref="submodelElement_abstract"/>
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:group name="submodelElement_t">
    <xs:sequence>
      <xs:group ref="referable_t"/>
      <xs:group ref="hasKind_t"/>
      <xs:group ref="hasSemantics_t"/>
      <xs:group ref="qualifiable_t"/>
      <xs:group ref="hasDataSpecification_t"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="submodelElement_abstract">
    <xs:choice>
      <xs:element name="submodelElementCollection" type="submodelElementCollection_t"/>
      <xs:element name="property" type="property_t"/>
      <xs:element name="multiLanguageProperty" type="multiLanguageProperty_t"/>
      <xs:element name="range" type="range_t"/>
      <xs:element name="referenceElement" type="referenceElement_t"/>
      <xs:element name="blob" type="blob_t"/>
      <xs:element name="file" type="file_t"/>
      <xs:element name="annotatedRelationshipElement" type="annotatedRelationshipElement_t"/>
      <xs:element name="entity" type="entity_t"/>
      <xs:element name="basicEvent" type="basicEvent_t"/>
      <xs:element name="operation" type="operation_t"/>
      <xs:element name="capability" type="capability_t"/>
    </xs:choice>
  </xs:group>
  <xs:group name="relationshipElement_t">
    <xs:sequence>
      <xs:group ref="submodelElement_t"/>
      <xs:element name="first" type="reference_t"/>
      <xs:element name="second" type="reference_t"/>
    </xs:sequence>
  </xs:group>
  <xs:complexType name="submodelElementCollection_t">
    <xs:sequence>
      <xs:group ref="submodelElement_t"/>
      <xs:element name="values" minOccurs="0">
        <xs:complexType>
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:group ref="submodelElement_abstract"/>
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:element name="ordered" type="xs:boolean" minOccurs="0"/>
      <xs:element name="allowDuplicates" type="xs:boolean" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:group name="dataElement_t">
    <xs:sequence>
      <xs:group ref="submodelElement_t"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="dataElement_abstract">
    <xs:choice>
      <xs:element name="property" type="property_t"/>
      <xs:element name="multiLanguageProperty" type="multiLanguageProperty_t"/>
      <xs:element name="range" type="range_t"/>
      <xs:element name="referenceElement" type="referenceElement_t"/>
      <xs:element name="blob" type="blob_t"/>
      <xs:element name="file" type="file_t"/>
    </xs:choice>
  </xs:group>
  <xs:complexType name="property_t">
    <xs:sequence>
      <xs:group ref="dataElement_t"/>
      <xs:element name="valueType" type="dataTypeDef_t"/>
      <xs:element name="value" type="xs:string" minOccurs="0"/>
      <xs:element name="valueId" type="reference_t" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="multiLanguageProperty_t">
    <xs:sequence>
      <xs:group ref="dataElement_t"/>
      <xs:element name="value" type="langStringSet_t" minOccurs="0"/>
      <xs:element name="valueId" type="reference_t" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="range_t">
    <xs:sequence>
      <xs:group ref="dataElement_t"/>
      <xs:element name="valueType" type="dataTypeDef_t"/>
      <xs:element name="min" type="xs:string" minOccurs="0"/>
      <xs:element name="max" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="referenceElement_t">
    <xs:sequence>
      <xs:group ref="dataElement_t"/>
      <xs:element name="value" type="reference_t" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="blob_t">
    <xs:sequence>
      <xs:group ref="dataElement_t"/>
      <xs:element name="mimeType" type="xs:string"/>
      <xs:element name="value" type="xs:base64Binary" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="file_t">
    <xs:sequence>
      <xs:group ref="dataElement_t"/>
      <xs:element name="mimeType" type="xs:string"/>
      <xs:element name="value" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="annotatedRelationshipElement_t">
    <xs:sequence>
      <xs:group ref="relationshipElement_t"/>
      <xs:element name="annotation" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="reference" type="reference_t"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="entityType_t">
    <xs:restriction base="xs:string">
      <xs:enumeration value="CoManagedEntity"/>
      <xs:enumeration value="SelfManagedEntity"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="entity_t">
    <xs:sequence>
      <xs:group ref="submodelElement_t"/>
      <xs:element name="entityType" type="entityType_t"/>
      <xs:element name="statements" minOccurs="0">
        <xs:complexType>
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:group ref="submodelElement_abstract"/>
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:element name="globalAssetId" type="reference_t" minOccurs="0"/>
      <xs:element name="specificAssetIds" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="identifierKeyValuePair" type="identifierKeyValuePair_t"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:group name="event_t">
    <xs:sequence>
      <xs:group ref="submodelElement_t"/>
    </xs:sequence>
  </xs:group>
  <xs:complexType name="basicEvent_t">
    <xs:sequence>
      <xs:group ref="event_t"/>
      <xs:element name="observed" type="reference_t"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="operation_t">
    <xs:sequence>
      <xs:group ref="submodelElement_t"/>
      <xs:element name="inputVariables" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="operationVariable" type="operationVariable_t"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="outputVariables" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="operationVariable" type="operationVariable_t"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="inoutputVariables" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="operationVariable" type="operationVariable_t"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="operationVariable_t">
    <xs:sequence>
      <xs:element name="value">
        <xs:complexType>
          <xs:choice>
            <xs:group ref="submodelElement_abstract"/>
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="capability_t">
    <xs:sequence>
      <xs:group ref="submodelElement_t"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="conceptDescription_t">
    <xs:sequence>
      <xs:group ref="identifiable_t"/>
      <xs:group ref="hasDataSpecification_t"/>
      <xs:element name="isCaseOf" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="reference" type="reference_t"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="view_t">
    <xs:sequence>
      <xs:group ref="referable_t"/>
      <xs:group ref="hasSemantics_t"/>
      <xs:group ref="hasDataSpecification_t"/>
      <xs:element name="containedElements" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="reference" type="reference_t"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="reference_t">
    <xs:sequence>
      <xs:element name="keys">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="1" maxOccurs="unbounded" name="key" type="key_t"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="key_t">
    <xs:sequence>
      <xs:element name="type" type="keyElements_t"/>
      <xs:element name="value" type="xs:string"/>
      <xs:element name="idType" type="keyType_t"/>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="identifiableElements_t">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Asset"/>
      <xs:enumeration value="AssetAdministrationShell"/>
      <xs:enumeration value="ConceptDescription"/>
      <xs:enumeration value="Submodel"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="referableElements_t">
    <xs:restriction base="xs:string">
      <xs:enumeration value="AccessPermissionRule"/>
      <xs:enumeration value="AnnotatedRelationshipElement"/>
      <xs:enumeration value="Asset"/>
      <xs:enumeration value="AssetAdministrationShell"/>
      <xs:enumeration value="BasicEvent"/>
      <xs:enumeration value="Blob"/>
      <xs:enumeration value="Capability"/>
      <xs:enumeration value="ConceptDescription"/>
      <xs:enumeration value="ConceptDictionary"/>
      <xs:enumeration value="DataElement"/>
      <xs:enumeration value="Entity"/>
      <xs:enumeration value="Event"/>
      <xs:enumeration value="File"/>
      <xs:enumeration value="MultiLanguageProperty"/>
      <xs:enumeration value="Operation"/>
      <xs:enumeration value="Property"/>
      <xs:enumeration value="Range"/>
      <xs:enumeration value="ReferenceElement"/>
      <xs:enumeration value="RelationshipElement"/>
      <xs:enumeration value="Submodel"/>
      <xs:enumeration value="SubmodelElement"/>
      <xs:enumeration value="SubmodelElementCollection"/>
      <xs:enumeration value="View"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="keyElements_t">
    <xs:restriction base="xs:string">
      <xs:enumeration value="GlobalReference"/>
      <xs:enumeration value="FragmentReference"/>
      <xs:enumeration value="AccessPermissionRule"/>
      <xs:enumeration value="AnnotatedRelationshipElement"/>
      <xs:enumeration value="Asset"/>
      <xs:enumeration value="AssetAdministrationShell"/>
      <xs:enumeration value="BasicEvent"/>
      <xs:enumeration value="Blob"/>
      <xs:enumeration value="Capability"/>
      <xs:enumeration value="ConceptDescription"/>
      <xs:enumeration value="ConceptDictionary"/>
      <xs:enumeration value="DataElement"/>
      <xs:enumeration value="Entity"/>
      <xs:enumeration value="Event"/>
      <xs:enumeration value="File"/>
      <xs:enumeration value="MultiLanguageProperty"/>
      <xs:enumeration value="Operation"/>
      <xs:enumeration value="Property"/>
      <xs:enumeration value="Range"/>
      <xs:enumeration value="ReferenceElement"/>
      <xs:enumeration value="RelationshipElement"/>
      <xs:enumeration value="Submodel"/>
      <xs:enumeration value="SubmodelElement"/>
      <xs:enumeration value="SubmodelElementCollection"/>
      <xs:enumeration value="View"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="localKeyType_t">
    <xs:restriction base="xs:string">
      <xs:enumeration value="IdShort"/>
      <xs:enumeration value="FragmentId"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="keyType_t">
    <xs:restriction base="xs:string">
      <xs:enumeration value="IdShort"/>
      <xs:enumeration value="FragmentId"/>
      <xs:enumeration value="IRDI"/>
      <xs:enumeration value="IRI"/>
      <xs:enumeration value="Custom"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="dataTypeDef_t">
    <xs:restriction base="xs:string">
